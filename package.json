{
  "name": "cezerin2-client",
  "version": "0.36.0",
  "description": "Cezerin2 API client library. Work on browser and server.",
  "keywords": [
    "cezerin2",
    "cezerin",
    "cezerin api",
    "cezerin ajax",
    "cezerin web store",
    "ecommerce api",
    "shopping cart api"
  ],
  "homepage": "https://cezerin.com",
  "bugs": "https://github.com/cezerin2/client/issues",
  "license": "MIT",
  "author": {
    "name": "Restmount <jsonmount@gmail.com>",
    "url": "https://github.com/restmount"
  },
  "contributors": [
    {
      "name": "Luke Busstra <luke.busstra@gmail.com>",
      "url": "https://github.com/lubuss"
    },
    {
      "name": "Restmount <jsonmount@gmail.com>",
      "url": "https://github.com/restmount"
    },
    {
      "name": "Sergey Onufrienko <sergey@onufrienko.com>",
      "url": "https://github.com/sonufrienko"
    }
  ],
  "source": "src/index.ts",
  "main": "lib/main.js",
  "module": "lib/module.js",
  "types": "lib/types.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/cezerin2/client"
  },
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "prepublishOnly": "npm run build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier --write \"src/**/*.js\"",
    "precommit": "lint-staged",
    "test": "yarn build && c8 ava",
    "test:watch": "yarn build && ava --watch"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "reporters": [
      "default",
      "jest-junit"
    ],
    "transform": {
      "^.+\\.m?js$": "babel-jest"
    }
  },
  "jest-junit": {
    "output": "reports/junit/junit.xml",
    "usePathForSuiteName": "true"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cross-fetch": "^3.1.5",
    "query-string": "^8.0.3"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@parcel/packager-ts": "^2.6.2",
    "@parcel/transformer-typescript-types": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "ava": "^4.3.1",
    "c8": "^7.12.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "parcel": "^2.6.2",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
